msgstr "Đã khôi phục cấu hình từ bản sao lưu. Đang tải lại..."

msgid "Restore failed: %s"
msgstr "Khôi phục không thành công: %s"

msgid "Backup file is corrupted: %s"
msgstr "Tệp sao lưu bị hỏng: %s"

msgid "Error reading backup: %s"
msgstr "Lỗi đọc bản sao lưu: %s"

msgid "Restore backup .bak"
msgstr "Khôi phục bản sao lưu .bak"

msgid "Change port state"
msgstr "Thay đổi trạng thái cổng"

