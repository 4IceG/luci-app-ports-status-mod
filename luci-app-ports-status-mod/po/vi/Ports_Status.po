msgstr "Content-Type: text/plain; charset=UTF-8"

msgid "Edit Port Label"
msgstr "Chỉnh sửa nhãn cổng"

msgid "Enter new label for this port:"
msgstr "Nhập nhãn mới cho cổng này:"

msgid "Device"
msgstr "Thiết bị"

msgid "Original label"
msgstr "Nhãn gốc"

msgid "Restore Original"
msgstr "Khôi phục gốc"

msgid "Cancel"
msgstr "Hủy"

msgid "Save"
msgstr "Lưu"

msgid "Port configuration saved successfully"
msgstr "Đã lưu cấu hình cổng thành công"

msgid "Port order saved"
msgstr "Đã lưu thứ tự cổng"

msgid "Click to edit label"
msgstr "Nhấp để chỉnh sửa nhãn"

msgid "Hold to drag and reorder"
msgstr "Giữ để kéo và sắp xếp lại"

msgid "Label max 9 chars, description max 50 chars."
msgstr "Nhãn tối đa 9 ký tự, mô tả tối đa 50 ký tự."

msgid "Enter description (optional):"
msgstr "Nhập mô tả (tùy chọn):"

msgid "Optional description"
msgstr "Mô tả tùy chọn"

msgid "User settings are saved to the /etc/user_defined_ports.json file."
msgstr "Cài đặt người dùng được lưu vào tệp /etc/user_defined_ports.json."

msgid "Configuration file downloaded"
msgstr "Đã tải xuống tệp cấu hình"

msgid "Configuration file not found"
msgstr "Không tìm thấy tệp cấu hình"

msgid "Download error: %s"
msgstr "Lỗi tải xuống: %s"

