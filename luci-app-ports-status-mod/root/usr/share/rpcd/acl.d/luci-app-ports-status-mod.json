{
  "luci-ports": {
    "description": "Access to custom Ports Status",
    "read": {
      "uci": [ "*" ],
      "ubus": {
        "luci": [ "getBuiltinEthernetPorts" ],
        "file": [ "read", "write" ]
      },
      "file": {
        "/etc/user_defined_ports.json": [ "read" ],
        "/www/luci-static/resources/view/status/include/29_ports.js": [ "read" ]
      }
    },
    "write": {
      "file": {
        "/etc/user_defined_ports.json": [ "write" ]
      },
      "ubus": {
        "file": [ "write" ],
        "luci": [ "getBuiltinEthernetPorts" ]
      }
    }
  }
}
